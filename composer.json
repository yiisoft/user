{
    "name": "yiisoft/user",
    "type": "library",
    "description": "",
    "keywords": [
        "yiisoft",
        "user"
    ],
    "homepage": "https://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/user/issues?state=open",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/user"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "yiisoft/access": "dev-master",
        "yiisoft/auth": "^1.0",
        "yiisoft/cookies": "dev-master",
        "yiisoft/session": "dev-master",
        "psr/log": "^1.1",
        "psr/event-dispatcher": "^1.0",
        "psr/http-factory": "^1.0"
    },
    "require-dev": {
        "nyholm/psr7": "^1.0",
        "infection/infection": "^0.18.2",
        "phpunit/phpunit": "^9.4",
        "vimeo/psalm": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\User\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\User\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin": {
            "web": "config/web.php"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": [
            "phpunit --colors --no-interaction"
        ]
    }
}
